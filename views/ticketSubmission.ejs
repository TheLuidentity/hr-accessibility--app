<%- include("header") -%>

<div
  class="submission-page-body"
  role="group"
  aria-label="ticket submission page"
>
  <h2>Ticket Submission</h2>

  <p>Fields marked with (*) are required</p>

  <form id="ticket-submission-form" name="ticket-submission-form">
    <label
      id="reason-for-ticket"
      name="reason-for-ticket"
      class="input-label"
      for="reason-for-ticket-input"
      >Reason for ticket submission
      <span aria-hidden="true" class="red">*</span>
    </label>
    <select id="reason-for-ticket-input" name="select" autofocus required>
      <option></option>
      <% for (let item of ticketSubmissionChoices) { %>
      <option><%= item.title %></option>
      <% } %>
    </select>

    <label
      id="first-name"
      name="first-name"
      class="input-label"
      for="first-name-input"
      aria-label="first name required"
    >
      First Name<span aria-hidden="true" class="red">*</span>
    </label>
    <input id="first-name-input" type="text" required />
    <label
      id="last-name"
      name="last-name"
      class="input-label"
      for="last-name-input"
      aria-label="last name required"
    >
      Last Name <span aria-hidden="true" class="red">*</span>
    </label>
